
// PURPOSE:Control input data for keccak module
// TODO:finish program
// byte_num

// synopsys translate_off
`include "timescale.v"
// synopsys translate_on
`include "or1200_defines.v"

module keccak_ctrl(clk,rst,in,keccak_en,a);
   input clk, rst;
   input in;//from alu "out32"
   input keccak_en;

   output byte_num;
   
   assign byte_num = 2'b11; //Fix byte_num = 3 temporarily
   always @ (posedge clk)
     if(keccak_en)begin
	in_ready = 1;
     end
     else
       in_ready = 0;
   



endmodule
